[ignore]

[untyped]
.*/node_modules/*

[include]

[libs]
flow/

[lints]

[options]
# map .graphql files to an empty object, see https://github.com/facebook/flow/issues/2852
module.name_mapper='.*\(.graphql\)' -> 'empty/object'
module.name_mapper='annotator_range' -> 'empty/object'
module.name_mapper.extension='scss' -> 'empty/object'
